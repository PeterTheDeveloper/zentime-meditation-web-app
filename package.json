{
  "name": "meditation-web-app",
  "version": "1.0.31",
  "description": "Simple meditation timer web app built with React.js and mobX.",
  "main": "webpack.config.js",
  "engines": {
    "node": ">=9.0",
    "npm": ">=5.0",
    "yarn": ">=1.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js --mode development",
    "build": "cross-env NODE_ENV=production rimraf ./dist && webpack --config webpack.config.production.js --mode production",
    "postinstall": "webpack --config ./webpack.config.production.js --display-error-details --mode production",
    "preview": "cross-env NODE_ENV=preview http-server ./dist -p 1234 -c-1",
    "test": "cross-env NODE_ENV=test echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexdevero/meditation-web-app.git"
  },
  "keywords": [
    "app",
    "ES6",
    "ES7",
    "meditation",
    "mindfulness",
    "mobx",
    "react",
    "reactjs",
    "web app",
    "Webpack"
  ],
  "author": "Alex Devero <deveroalex@gmail.com> (http://github.com/alexdevero)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexdevero/meditation-web-app/issues"
  },
  "homepage": "",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bootstrap": "4.0.0-alpha.6",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.10",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^3.0.6",
    "image-webpack-loader": "^4.1.0",
    "node-sass": "^4.7.2",
    "normalize.css": "^8.0.0",
    "postcss-loader": "^2.1.1",
    "react-hot-loader": "4.0.0",
    "resolve-url-loader": "^2.3.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.11",
    "webpack-dev-server": "^3.1.1"
  },
