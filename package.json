    "axios": "^0.18.0",
    "css-declaration-sorter": "^3.0.1",
    "css-mqpacker": "^6.0.2",
    "faker": "^4.1.0",
    "file-loader": "^1.1.11",
    "http-server": "^0.11.1",
    "lazy-route": "^1.0.7",
    "mobx": "^3.6.1",
    "mobx-react": "^4.4.3",
    "mobx-react-devtools": "^4.2.15",
    "mobx-react-router": "^4.0.1",
    "moment": "^2.21.0",
    "package": "^1.0.1",
    "postcss-cssnext": "^3.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
